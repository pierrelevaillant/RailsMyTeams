<%= form_for [:admin, @team] do |f| %>
	<% if notice %>
    <div class="panel"><%= notice %></div>
    <% end %>

	<%= f.label :team_name %>
	<%= f.text_field :team_name, class: 'form-control' %>

	<%= select("player", "category_id", Player.all.collect {|p| [ p.first_name + " " + p.last_name, p.id ] }) %>

	<%= f.submit "Save changes", class: "btn btn-primary" %>
<% end %>
